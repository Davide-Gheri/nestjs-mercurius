{
  "name": "nestjs-mercurius",
  "version": "0.10.1",
  "main": "index.js",
  "repository": "https://@github.com:Davide-Gheri/nestjs-mercurius.git",
  "author": "Davide Gheri <davide@davidegheri.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "lint": "eslint 'lib/**/*.ts'",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it",
    "test": "uvu -r ts-node/register tests/e2e",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@apollo/federation": "^0.23.2",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@graphql-tools/utils": "^7.10.0",
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/testing": "^7.6.18",
    "@release-it/conventional-changelog": "^3.0.1",
    "@types/graphql-upload": "^8.0.5",
    "@types/jest": "^26.0.22",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.22.1",
    "altair-fastify-plugin": "^3.2.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.4",
    "fastify": "^3.15.1",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.10.0",
    "graphql-tag": "^2.11.0",
    "husky": "7",
    "lint-staged": "^11.0.0",
    "mercurius": "^7.9.1",
    "mercurius-integration-testing": "^3.1.3",
    "mercurius-upload": "^1.1.1",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^14.10.0",
    "rxjs": "^6.6.6",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "uvu": "^0.5.1",
    "ws": "^7.5.1"
  },
  "dependencies": {
    "@nestjs/graphql": "~7.11.0"
  },
  "peerDependencies": {
    "@apollo/federation": "^0.23.2",
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-fastify": "^7.6.18",
    "altair-fastify-plugin": "^3.2.2",
    "fastify": "^3.15.1",
    "graphql": "^15.5.1",
    "mercurius": "^7.7.0",
    "mercurius-upload": "^1.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
