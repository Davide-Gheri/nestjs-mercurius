{
  "name": "nestjs-mercurius",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://@github.com:Davide-Gheri/nestjs-mercurius.git",
  "author": "Davide Gheri <davide@davidegheri.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it",
    "test": "uvu -r ts-node/register tests/e2e"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/platform-fastify": "^7.6.12",
    "@nestjs/testing": "^7.6.12",
    "@types/jest": "^26.0.20",
    "@types/supertest": "^2.0.10",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "fastify": "^3.11.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "mercurius": "^7.0.0",
    "mercurius-integration-testing": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "@nestjs/graphql": "^7.9.8"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/platform-fastify": "^7.6.12",
    "fastify": "^3.11.0",
    "graphql": "^15.5.0",
    "mercurius": "^7.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
